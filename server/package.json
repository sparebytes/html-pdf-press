{
  "name": "@html-pdf-press/server",
  "version": "0.3.0",
  "description": "",
  "author": "Franklin Davenport",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sparebytes/html-pdf-press.git"
  },
  "bin": {
    "htmlpdfpress": "./cli.js"
  },
  "scripts": {
    "build": "rimraf dist && ttsc -p tsconfig.build.json && cpx ../README.md dist && package-json-reducer -s \"scripts devDependencies jest\" -o dist/package.json package.json && cpx {config/default.json,config/custom-environment-variables.json} dist",
    "start": "ts-node --compiler ttypescript -r tsconfig-paths/register src/main.ts",
    "start:dev": "tsc-watch --compiler ttypescript/bin/tsc -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "tsc-watch --compiler ttypescript/bin/tsc -p tsconfig.build.json --onSuccess \"node --inspect=127.0.0.1:7354 dist/main.js\"",
    "start:break": "tsc-watch --compiler ttypescript/bin/tsc -p tsconfig.build.json --onSuccess \"node --inspect=127.0.0.1:7354 --inspect-brk dist/main.js\"",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@html-pdf-press/puppeteer-util": "^0.3.0",
    "@html-pdf-press/types": "^0.3.0",
    "@nestjs/common": "~6.6.7",
    "@nestjs/core": "~6.6.7",
    "@nestjs/platform-express": "~6.6.7",
    "@types/lodash": "^4.14.138",
    "config": "~3.2.2",
    "lodash": "^4.17.15",
    "puppeteer": "~1.19.0",
    "reflect-metadata": "~0.1.12",
    "rimraf": "~2.6.2",
    "rxjs": "~6.5.3"
  },
  "devDependencies": {
    "@nestjs/testing": "~6.6.7",
    "@types/config": "~0.0.34",
    "@types/express": "~4.17.1",
    "@types/jest": "~24.0.11",
    "@types/node": "~12.7.4",
    "@types/puppeteer": "~1.19.1",
    "@types/supertest": "~2.0.7",
    "cpx": "~1.5.0",
    "jest": "~24.7.1",
    "package-json-reducer": "~1.0.18",
    "standard-version": "^6.0.1",
    "supertest": "~4.0.2",
    "ts-jest": "~24.0.2",
    "ts-node": "~8.3.0",
    "tsc-watch": "~3.0.0",
    "tsconfig-paths": "~3.8.0",
    "tslint": "~5.19.0",
    "ttypescript": "~1.5.7",
    "typescript": "~3.6.2",
    "typesmith": "~0.9.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
